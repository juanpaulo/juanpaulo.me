            span.label Github
        li
          a.icon.circle.fa-twitter(href='http://twitter.com/jp_gutierrez17' target='_blank')
            span.label Twitter
        li
          a.icon.circle.fa-flickr(href='http://www.flickr.com/photos/juan_paulo/' target='_blank')
